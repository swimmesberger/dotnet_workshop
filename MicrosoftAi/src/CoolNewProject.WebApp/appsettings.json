{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "HttpClientResilience": {
        "Chatbot": {
            "Retry": {
                "MaxRetryAttempts": 1
            },
            "CircuitBreaker": {
                "SamplingDuration": "00:10:00"
            },
            "TotalRequestTimeout": {
                "Timeout": "00:10:00"
            },
            "AttemptTimeout": {
                "Timeout": "00:05:00"
            }
        },
        "Catalog": {}
    }
}
